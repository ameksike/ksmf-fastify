  "bugs": {
    "url": "https://github.com/ameksike/ksmf-fastify/issues"
  },
  "homepage": "https://github.com/ameksike/ksmf-fastify#readme",
  "devDependencies": {
    "jest": "^26.6.3"
  },
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/middie": "^8.3.0",
    "fastify": "^4.26.1",
    "ksmf": "^1.3.0",
    "serve-static": "^1.15.0"
